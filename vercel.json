{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/authorize",
        "dest": "/server.js"
      },
      {
        "src": "/credentials",
        "dest": "/server.js"
      },
      {
        "src": "/token",
        "dest": "/server.js"
      }
    ]
  }
  